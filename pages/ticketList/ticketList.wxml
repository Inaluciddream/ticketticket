<view class="ticket-list-container" hover-class="none" hover-stop-propagation="false">
    <scroll-view class="ticket-list" scroll-x="false" scroll-y="false" upper-threshold="50" lower-threshold="50" 
        scroll-top="0" scroll-left="0" scroll-into-view="" scroll-with-animation="false" enable-back-to-top="false" 
        bindscrolltoupper="" bindscrolltolower="" bindscroll="">
        <view class="single-ticket">
            <view class="ticket-left">
                <view class="ticket-name">
                    快递存放0.8元/次
                </view>
                <view class="ticket-time">
                    2019-01-01至2019-02-02
                </view>
            </view> 
            <view class="ticket-right">
                <view class="ticket-status status-1">
                    有效
                </view>
                <view class="ticket-num-info">
                    已领500 已用300
                </view>
            </view>
        </view>
    </scroll-view>
    
    <view class="create-ticket-btn">
        
    </view>
</view>
